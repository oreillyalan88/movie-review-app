<%= simple_form_for @movie , :html => {:multipart => true} do |f| %>
    <%= select_tag(:genre_id, options_for_select(@genres), :prompt => "Select a Genre")%>
    <%= f.input :title, label: "Movie Title" %>
    <%= f.input :description  %>
    <%= f.input :director %>
    <%= f.button :submit %>
<% end %>